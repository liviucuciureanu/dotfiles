Include config.d/*

Host ubisoft
  HostName localhost
  ProxyJump jump 
  Port 22222
  User pcuciureanu
  IdentityFile ~/.ssh/id_rsa_home
  ProxyCommand ssh -W %h:%p
  ForwardX11 yes

Host jump 
  HostName primary.lan
  User paul
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa
  IdentityFile ~/.ssh/id_ed25519

Host work
  HostName localhost
  ProxyJump ubisoft
  User paul
  Port 33333
  IdentityFile ~/.ssh/id_rsa_mtl-bj201
  ProxyCommand ssh -W %h:%p
  ForwardX11 yes

Host router
  HostName 192.168.1.1
  User root
  IdentityFile ~/.ssh/id_rsa
  IdentityFile ~/.ssh/id_ed25519

